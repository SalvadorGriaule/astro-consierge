---
import Layout from "../../layouts/Layout.astro";
const { id } = Astro.params;
const info = Astro.cookies.get("connect.sid")?.value
const sessionID  = info?.slice(info?.indexOf(':')+1,info?.indexOf('.'))
---

<Layout>
    <script>
        const cookies = document.cookie
        console.log(cookies);
    </script>
    <h2>{id}</h2>
    <div>
        <p>{info}</p>
        <p>{sessionID}</p>
    </div>
</Layout>